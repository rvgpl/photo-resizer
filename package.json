{
  "name": "@rvgpl/photo-resizer",
  "version": "1.0.0",
  "description": "A CLI app to resize photos",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "preferGlobal": true,
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "start": "node ./dist/index.js",
    "lint": "npx eslint . --ext '.js,.jsx,.ts,.tsx,.md'",
    "format": "npx prettier -c --write '**/*'",
    "test": "jest --config jest.config.json --no-cache",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "publish": "npm run build && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prepare"
    }
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "photos",
    "resize",
    "cli",
    "sharp",
    "typescript"
  ],
  "author": "Ravigopal Kesari <rvgpl>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvgpl/photo-resizer.git"
  },
  "bugs": {
    "url": "https://github.com/rvgpl/photo-resizer/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/rvgpl/photo-resizer",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.7",
    "@types/sharp": "^0.31.1",
    "@types/yargs": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "sharp": "^0.31.3",
    "yargs": "^17.7.1"
  }
}
